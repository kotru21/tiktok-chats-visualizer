{
  "name": "tiktok-chats-visualizer",
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "cross-env NODE_OPTIONS=\"--import tsx\" mocha \"tests/**/*.test.ts\" --exclude \"tests/colorScheme.test.ts\" --exclude \"tests/theme.test.ts\"",
    "test:js": "mocha \"tests/**/*.test.js\" --exclude \"tests/colorScheme.test.js\" --exclude \"tests/theme.test.js\"",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "typecheck:client": "tsc -p tsconfig.client.json --noEmit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "Анализатор и визуализатор чатов TikTok (без сервера)",
  "dependencies": {
    "chart.js": "^4.5.1"
  },
  "devDependencies": {
    "@types/jsdom": "^27.0.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.10.1",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "jsdom": "^27.2.0",
    "mocha": "^11.7.5",
    "prettier": "^3.7.4",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1",
    "vite": "^7.2.6"
  }
}
